@model Patment.Models.Staff
@{
    ViewBag.Title = "DetailsOfStaff";
}

<h2>Details Of Staff</h2>

<table class="table">
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Start Date</th>
        <th>Date Of Birth</th>
        <th>Staff Type ID</th>

    </tr>

    <tr>

        <td>@Model.FirstName</td>
        <td>@Model.LastName</td>
        <td>@Model.StartDate</td>
        <td>@Model.DateOfBirth</td>
        <td>@Model.StaffTypeID</td>
    </tr>
</table>


